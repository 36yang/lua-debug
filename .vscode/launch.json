{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lua",
            "request": "launch",
            "name": "Test IC",
            "program": "${workspaceFolder}/luabuild/main.lua",
            "path": [
                "${workspaceFolder}/luabuild/?.lua",
                "${workspaceFolder}/luabuild/?.lua",
            ],
            "cpath": "${workspaceFolder}/luabuild/?.dll",
            "env": {
                "PATH": "${workspaceFolder}/luabuild/"
            },
            "cwd": "${workspaceFolder}",
            "arg": [
                "Debug",
                "IC"
            ]
        },
        {
            "type": "lua",
            "request": "launch",
            "name": "Test Client",
            "console": "integratedTerminal",
            "experimentalServer": true,
            "luaRuntime": "5.4 32bit",
            "luaexe": "${env:USERPROFILE}/.vscode/extensions/actboy168.lua-debug-0.9.1/bin/win/lua-debug.exe",
            "luadll": "${env:USERPROFILE}/.vscode/extensions/actboy168.lua-debug-0.9.0/bin/win/lua54.dll",
            "program": "${workspaceFolder}/extension/script/frontend/main.lua",
            "path": "${workspaceFolder}/extension/script/?.lua",
            "arg": [
                "4711"
            ]
        },
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Test Extension",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/extension/"
            ]
        }
    ]
}
