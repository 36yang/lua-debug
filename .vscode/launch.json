{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lua",
            "request": "launch",
            "name": "Test IC",
            "program": "${workspaceFolder}/build/main.lua",
            "path": [
                "${workspaceFolder}/build/luabuild/?.lua",
                "${workspaceFolder}/build/?.lua",
            ],
            "cpath": "${workspaceFolder}/build/luabuild/?.dll",
            "env": {
                "PATH": "${workspaceFolder}/build/luabuild/"
            },
            "cwd": "${workspaceFolder}",
            "arg": [
                "Debug",
                "IC"
            ]
        },
        {
            "type": "lua",
            "request": "launch",
            "name": "Test Client",
            "luaexe": "${env:USERPROFILE}/.vscode/extensions/actboy168.lua-debug-0.5.1/client/win/bee.exe",
            "program": "${workspaceFolder}/extension/client/main.lua",
            "path": "${workspaceFolder}/extension/client/?.lua",
            "arg": [
                "26644"
            ]
        },
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Test Extension",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/extension/"
            ]
        }
    ]
}